<?xml version="1.0" encoding="utf-8"?>
<modules>

  <!-- Defaults -->

  <defaults>

    <module>
      <description>%(module/id)</description>
      <bugs-xmlrpc>http://bugzilla-test.gnome.org/xmlrpc.cgi</bugs-xmlrpc>
      <bugs-baseurl>http://bugzilla.gnome.org/</bugs-baseurl>
      <bugs-product>%(module/id)</bugs-product>
      <bugs-component>general</bugs-component>
      <scmroot type="svn">
        <path>http://svn.gnome.org/svn</path>
      </scmroot>
      <scmmodule>%(module/id)</scmmodule>
      <scmweb>http://svn.gnome.org/viewcvs/%(module/id)/trunk</scmweb>
    </module>

    <branch id="HEAD" regenerate="1">
      <scmweb>http://svn.gnome.org/viewcvs/%(module/id)/branches/%(branch/id)</scmweb>
      <domain id="po">
        <_description>UI translations</_description>
      </domain>
      <document id="help">
        <_description>User Guide</_description>
      </document>
    </branch>

  </defaults>


