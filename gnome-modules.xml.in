<?xml version="1.0" encoding="UTF-8"?>
<modules>

<defaults>
  <module>
    <bugs-xmlrpc>http://bugzilla-test.gnome.org/xmlrpc.cgi</bugs-xmlrpc>
    <bugs-baseurl>http://bugzilla.gnome.org/</bugs-baseurl>
    <bugs-product>%(module/id)</bugs-product>
    <bugs-component>general</bugs-component>
    <cvsroot>:pserver:anonymous@anoncvs.gnome.org:/cvs/gnome</cvsroot>
    <cvsmodule>%(module/id)</cvsmodule>
    <cvsweb>http://cvs.gnome.org/viewcvs/%(module/id)/</cvsweb>
  </module>
  <branch id="HEAD" regenerate="1">
    <cvs-web>http://cvs.gnome.org/viewcvs/%(module/id)/?only_with_tag=%(branch/id)</cvs-web>
    <domain id="po" potbase="%(module/id)">
      <_description>UI translations</_description>
    </domain>
    <document id="help">
      <_description>User Guide</_description>
    </document>
  </branch>
</defaults>

<module id="deskbar-applet">
  <description>Deskbar Applet</description>
  <webpage>http://raphael.slinckx.net/deskbar/</webpage>
  <maintainer id="rslinckx"/>
  <branch id="HEAD" />
</module>

<module id="epiphany">
  <description>Epiphany Web Browser</description>
  <webpage>http://gnome.org/projects/epiphany/</webpage>
  <maintainer id="chpe"/>
  <cvs-module>epiphany</cvs-module>
  <branch regenerate="0" id="gnome-2-12">
    <domain id="po">
      <description>UI translations</description>
    </domain>
    <document id="help">
      <description>User Guide</description>
      <directory>help</directory>
    </document>
  </branch>
  <branch stringfrozen="true" id="HEAD">
    <domain id="po">
      <description>UI translations</description>
    </domain>
    <document id="help">
      <description>Epiphany Manual</description>
      <directory>help</directory>
    </document>
  </branch>
</module>

<module id="gnome-applets">
  <description>Gnome Applets</description>
  <maintainer id="davyd"/>
  <defaults>
    <branch>
      <domain id="po">
	<description>UI translations</description>
      </domain>
      <domain id="po-locations" potbase="weather-locations">
	<description>Weather Applet Locations</description>
      </domain>
      <document id="accessx-status">
	<description>Keyboard Accessibility Monitor Manual</description>
	<directory>accessx-status/docs</directory>
      </document>
      <document id="battstat">
	<description>Battery Charage Monitor Manual</description>
	<directory>battstat/docs</directory>
      </document>
      <document id="char-palette">
	<description>Character Palette Manual</description>
	<directory>charpick/help</directory>
      </document>
      <document id="cpufreq-applet">
	<description>CPU Frequency Scaling Monitor Manual</description>
	<directory>cpufreq/help</directory>
      </document>
      <document id="drivemount">
	<description>Disk Mounter Manual</description>
	<directory>drivemount/help</directory>
      </document>
      <document id="geyes">
	<description>Geyes Manual</description>
	<directory>geyes/docs</directory>
      </document>
      <document id="gswitchit">
	<description>Keyboard Indicator Manual</description>
	<directory>gswitchit/help</directory>
      </document>
      <document id="gtik2_applet2">
	<description>Stock Ticker Manual</description>
	<directory>gtik/help</directory>
      </document>
      <document id="gweather">
	<description>Weather Report Manual</description>
	<directory>gweather/docs</directory>
      </document>
      <document id="command-line">
	<description>Command Line Manual</description>
	<directory>mini-commander/help</directory>
      </document>
      <document id="mixer_applet2">
	<description>Volume Control Manual</description>
	<directory>mixer/docs</directory>
      </document>
      <document id="multiload">
	<description>System Monitor Manual</description>
	<directory>multiload/docs</directory>
      </document>
      <document id="stickynotes_applet">
	<description>Sticky Notes Manual</description>
	<directory>stickynotes/docs</directory>
      </document>
      <document id="trashapplet">
	<description>Panel Trash Manual</description>
	<directory>trashapplet/docs</directory>
      </document>
    </branch>
  </defaults>
  <branch stringfrozen="true" id="HEAD" />
</module>

<module id="glib">
  <description>GLib</description>
  <webpage>http://gtk.org/</webpage>
  <maintainer id="matthiasc"/>
  <maintainer id="owen"/>
  <svnmodule>glib</svnmodule>
  <svnroot>http://svn.gnome.org/svn</svnroot>
  <svnweb>http://svn.gnome.org/viewsvn/%(module/id)</svnweb>
  <branch id="HEAD"/>
  <branch stringfrozen="true" id="glib-2-10"/>
</module>

<module id="gtk+">
  <description>Gtk+</description>
  <maintainer id="owen"/>
  <webpage>http://www.gtk.org/</webpage>
  <branch stringfrozen="true" id="gtk-2-8">
    <domain id="po">
      <description>Core</description>
    </domain>
    <domain id="po-properties" potbase="gtk+-properties">
      <description>Property Nicks</description>
    </domain>
  </branch>
  <branch id="HEAD">
    <domain id="po">
      <description>Core</description>
    </domain>
    <domain id="po-properties">
      <description>Property Nicks</description>
    </domain>
  </branch>
</module>

<module id="rhythmbox">
  <description>Rhythmbox Music Player</description>
  <webpage>http://www.rhythmbox.org/</webpage>
  <maintainer id="jrl"/>
  <branch id="HEAD"/>
</module>

<module id="sabayon">
  <description>Sabayon</description>
  <maintainer id="veillard"/>
  <branch id="HEAD"/>
</module>

<module id="xkeyboard-config">
  <description>XKeyboard-Config</description>
  <webpage>http://freedesktop.org/wiki/Software/XKeyboardConfig</webpage>
  <maintainer id="svu"/>
  <maintainer id="xdg-ivan"/>
  <bugs-baseurl>https://bugs.freedesktop.org/</bugs-baseurl>
  <bugs-product>xkeyboard-config</bugs-product>
  <bugs-component>General</bugs-component>
  <cvs-root>:pserver:anoncvs@cvs.freedesktop.org:/cvs/xlibs</cvs-root>
  <cvs-module>xkbdesc</cvs-module>
  <cvs-web>http://cvs.freedesktop.org/xlibs/xkbdesc/?only_with_id=%(branch)</cvs-web>
  <branch id="HEAD">
    <domain id="po">
      <description>Layout Descriptions</description>
    </domain>
  </branch>
</module>

</modules>
