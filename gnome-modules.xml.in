  <module id="gtk+">
    <!-- GNU's Not Unix Image Manipulation Program ToolKit+ ;) -->
    <_description>Gtk+</_description><!-- defaults to ID -->
    <maintainers>
	<maintainer><name>Owen Taylor</name>
		    <email>otaylor@redhat.com</email>
		    <webpage>http://people.redhat.com/otaylor/</webpage>
		    <hackergotchi>http://planet.gnome.org/heads/owen3.png</hackergotchi>
		    <irc-nickname>otaylor</irc-nickname></maintainer>
    </maintainers>
    <webpage>http://www.gtk.org/</webpage>
    <branches>
        <branch tag="gtk-2-8" stringfrozen="1">
            <translation-domains><!-- defaults to a single "po" -->
        	<domain base="gtk+">
        	    <directory>po</directory>
        	    <_description>Core</_description>
                </domain>
        	<domain base="gtk+-properties">
        	    <directory>po-properties</directory>
        	    <_description>Property Nicks</_description>
                </domain>
            </translation-domains>
	</branch>
        <branch tag="HEAD" />
    </branches>
  </module>

